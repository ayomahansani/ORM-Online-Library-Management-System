<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="adminSignUpPage" prefHeight="525.0" prefWidth="600.0" style="-fx-background-color: pink;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.controller.admin.AdminRegisterFormController">
    <children>
        <Label layoutX="193.0" layoutY="32.0" prefHeight="38.0" prefWidth="215.0" text="Admin Sign Up">
            <font>
                <Font name="Times New Roman" size="33.0" />
            </font>
        </Label>
        <TextField fx:id="txtName" layoutX="105.0" layoutY="139.0" onAction="#txtNameOnAction" prefHeight="35.0" prefWidth="376.0">
         <effect>
            <DropShadow color="#00000034" height="13.0" offsetX="5.0" offsetY="5.0" radius="6.0" width="13.0" />
         </effect></TextField>
        <TextField fx:id="txtEmail" layoutX="105.0" layoutY="215.0" onAction="#txtEmailOnAction" prefHeight="35.0" prefWidth="376.0">
         <effect>
            <DropShadow color="#00000033" height="13.0" offsetX="5.0" offsetY="5.0" radius="6.0" width="13.0" />
         </effect></TextField>
        <Label layoutX="107.0" layoutY="115.0" prefHeight="21.0" prefWidth="120.0" text="Name">
         <font>
            <Font name="Times New Roman" size="16.0" />
         </font></Label>
        <Label layoutX="107.0" layoutY="191.0" prefHeight="21.0" prefWidth="120.0" text="Email">
         <font>
            <Font name="Times New Roman" size="16.0" />
         </font></Label>
        <TextField fx:id="txtPassword" layoutX="105.0" layoutY="290.0" onAction="#txtPasswordOnAction" prefHeight="35.0" prefWidth="376.0">
         <effect>
            <DropShadow color="#00000034" height="13.0" offsetX="5.0" offsetY="5.0" radius="6.0" width="13.0" />
         </effect></TextField>
        <Label layoutX="107.0" layoutY="266.0" prefHeight="21.0" prefWidth="120.0" text="Password">
         <font>
            <Font name="Times New Roman" size="16.0" />
         </font></Label>
        <Button fx:id="btnRegister" layoutX="201.0" layoutY="374.0" mnemonicParsing="false" onAction="#btnRegisterOnAction" prefHeight="38.0" prefWidth="184.0" text="Register">
            <font>
                <Font name="Times New Roman" size="23.0" />
            </font>
         <effect>
            <DropShadow color="#0000004c" height="15.0" offsetX="7.0" offsetY="7.0" radius="7.0" width="15.0" />
         </effect>
        </Button>
      <Label layoutX="107.0" layoutY="474.0" prefHeight="19.0" prefWidth="184.0" text="Already have an account ?">
         <font>
            <Font name="Times New Roman" size="15.0" />
         </font>
      </Label>
      <Button fx:id="btnLoginHere" layoutX="385.0" layoutY="470.0" mnemonicParsing="false" onAction="#btnLoginHereOnAction" style="-fx-background-color: transparent;" text="Login Here" underline="true">
         <font>
            <Font name="Times New Roman" size="14.0" />
         </font></Button>
    </children>
</AnchorPane>
